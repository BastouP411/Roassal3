Class {
	#name : #RSForceBasedLayoutTest,
	#superclass : #RSTest,
	#instVars : [
		'canvas',
		'boxes'
	],
	#category : #'Roassal3-Global-Tests-Layouts'
}

{ #category : #running }
RSForceBasedLayoutTest >> setUp [
	super setUp.
	canvas := RSCanvas new.
	boxes := RSBox models: (10 to: 50).
	RSLineBuilder line
		canvas: canvas;
		shapes: boxes;
		color: (Color blue alpha: 0.5);
		connectFrom: [ :nb | nb // 2 ].
	canvas addAll: boxes.
]

